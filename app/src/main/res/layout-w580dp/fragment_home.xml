<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.studytor.app.fragments.ProfileFragment">
    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        xmlns:android="http://schemas.android.com/apk/res/android">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingTop="@dimen/card_margin_bigger"
            android:paddingBottom="@dimen/card_margin_bigger"
            android:paddingLeft="@dimen/card_margin"
            android:paddingRight="@dimen/card_margin">
            <RelativeLayout
                android:layout_height="match_parent"
                android:layout_width="0dp"
                android:layout_weight="1">
                <android.support.v7.widget.CardView
                    android:id="@+id/card1a"
                    android:layout_width="match_parent"
                    android:layout_height="350dp"
                    card_view:contentPadding="@dimen/card_padding"
                    android:layout_marginTop="@dimen/card_margin_bigger"
                    android:layout_marginBottom="@dimen/card_margin_bigger"
                    android:layout_marginLeft="@dimen/card_margin"
                    android:layout_marginRight="@dimen/card_margin"
                    card_view:cardCornerRadius="2dp">
                    <TextView
                        android:id="@+id/text1todelete"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="@string/not_support_home" />
                </android.support.v7.widget.CardView>
                <android.support.v7.widget.CardView
                    android:id="@+id/card2a"
                    android:layout_width="match_parent"
                    android:layout_height="200dp"
                    card_view:contentPadding="@dimen/card_padding"
                    android:layout_marginTop="@dimen/card_margin_bigger"
                    android:layout_marginBottom="@dimen/card_margin_bigger"
                    android:layout_marginLeft="@dimen/card_margin"
                    android:layout_marginRight="@dimen/card_margin"
                    card_view:cardCornerRadius="2dp"
                    android:layout_below="@id/card1a">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="@string/not_support_home" />
                </android.support.v7.widget.CardView>
                <android.support.v7.widget.CardView
                    android:id="@+id/card3a"
                    android:layout_width="match_parent"
                    android:layout_height="350dp"
                    card_view:contentPadding="@dimen/card_padding"
                    android:layout_marginTop="@dimen/card_margin_bigger"
                    android:layout_marginBottom="@dimen/card_margin_bigger"
                    android:layout_marginLeft="@dimen/card_margin"
                    android:layout_marginRight="@dimen/card_margin"
                    card_view:cardCornerRadius="2dp"
                    android:layout_below="@id/card2a">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="@string/not_support_home" />
                </android.support.v7.widget.CardView>
            </RelativeLayout>
            <RelativeLayout
                android:layout_height="match_parent"
                android:layout_width="0dp"
                android:layout_weight="1">
                <android.support.v7.widget.CardView
                    android:id="@+id/card1b"
                    android:layout_width="match_parent"
                    android:layout_height="250dp"
                    card_view:contentPadding="@dimen/card_padding"
                    android:layout_marginTop="@dimen/card_margin_bigger"
                    android:layout_marginBottom="@dimen/card_margin_bigger"
                    android:layout_marginLeft="@dimen/card_margin"
                    android:layout_marginRight="@dimen/card_margin"
                    card_view:cardCornerRadius="2dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="@string/not_support_home" />
                </android.support.v7.widget.CardView>
                <android.support.v7.widget.CardView
                    android:id="@+id/card2b"
                    android:layout_width="match_parent"
                    android:layout_height="350dp"
                    card_view:contentPadding="@dimen/card_padding"
                    android:layout_marginTop="@dimen/card_margin_bigger"
                    android:layout_marginBottom="@dimen/card_margin_bigger"
                    android:layout_marginLeft="@dimen/card_margin"
                    android:layout_marginRight="@dimen/card_margin"
                    card_view:cardCornerRadius="2dp"
                    android:layout_below="@id/card1b">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="@string/not_support_home" />
                </android.support.v7.widget.CardView>
                <android.support.v7.widget.CardView
                    android:id="@+id/card3b"
                    android:layout_width="match_parent"
                    android:layout_height="250dp"
                    card_view:contentPadding="@dimen/card_padding"
                    android:layout_marginTop="@dimen/card_margin_bigger"
                    android:layout_marginBottom="@dimen/card_margin_bigger"
                    android:layout_marginLeft="@dimen/card_margin"
                    android:layout_marginRight="@dimen/card_margin"
                    card_view:cardCornerRadius="2dp"
                    android:layout_below="@id/card2b">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="@string/not_support_home" />
                </android.support.v7.widget.CardView>
            </RelativeLayout>
        </LinearLayout>
    </ScrollView>
</FrameLayout>
